{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "deep-link:default",
    "updater:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://osureffer.vilaz.dev"
        }
      ]
    }
  ]
}